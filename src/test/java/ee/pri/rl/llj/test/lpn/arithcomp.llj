:- module(ee:pri:rl:llj:test:lpn:arithcomp, [
	test_all/0
]).

:- import(llj:lang).

test_all:-
	2 < 4,
	2 =< 4,
	4 =< 4,
	4 =:= 4,
	4 =\= 5,
	not(4 =\= 4),
	4 >= 4,
	4 > 2,
	2 < 4 + 1,
	2 + 1 < 4,
	2 + 1 < 3 + 2,
	2 + 2 =:= 4,
	not(4 < 2).